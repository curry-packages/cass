{
    "name": "cass",
    "version": "0.0.1",
    "author": "Michael Hanus <mh@informatik.uni-kiel.de>",
    "synopsis": "CASS: the Curry Analysis Server System",
    "category": [ "Analysis" ],
    "dependencies": {
        "cass-analysis": ">= 0.0.1",
        "flatcurry"    : ">= 1.0.0"
    },
    "compilerCompatibility": {
        "pakcs": ">= 1.14.0, < 2.0.0",
        "kics2": ">= 0.5.0, < 2.0.0"
    },
    "configModule": "CASS.PackageConfig",
    "executable": {
        "name": "cass",
        "main": "CASS.Main"
    },
    "documentation": {
        "src-dir": "docs",
        "main": "main.tex"
    },
    "source": {
        "git": "https://git.ps.informatik.uni-kiel.de/curry-packages/cass.git",
        "tag": "$version"
    }
}
